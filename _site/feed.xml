    <entry>
      <title type="html">交互原型：Social VR 隐私与身份管理</title>
      <link href="http://localhost:4000/" rel="alternate" type="text/html" title="交互原型：Social VR 隐私与身份管理" />
      <published>2024-10-01T00:00:00+02:00</published>
      <updated>2024-10-01T00:00:00+02:00</updated>
      <id>http://localhost:4000/social-vr-idm</id>
      <content type="html" xml:base="http://localhost:4000/">&lt;p&gt;阿是老师打死了&lt;/p&gt;

&lt;p&gt;&lt;img src=&quot;img/post-social-vr-idm/teaser.png&quot; alt=&quot;社交VR身份管理示意图&quot; width=&quot;100%&quot; /&gt;&lt;/p&gt;

&lt;p&gt;aldhaslj&lt;/p&gt;

&lt;div id=&quot;video-player-container&quot; style=&quot;max-width:700px;margin:auto;&quot;&gt;
  &lt;!-- 默认显示YouTube --&gt;
  &lt;div id=&quot;video-player&quot; style=&quot;margin-bottom:0.5em;&quot;&gt;
    &lt;iframe id=&quot;youtube-player&quot; src=&quot;https://www.youtube.com/embed/xxxxxxxxx&quot; width=&quot;100%&quot; height=&quot;400&quot; frameborder=&quot;0&quot; allowfullscreen=&quot;&quot;&gt;&lt;/iframe&gt;
  &lt;/div&gt;
  &lt;div style=&quot;text-align:right; font-size:0.9em; color:#666;&quot;&gt;
    &lt;label for=&quot;video-source&quot; style=&quot;margin-right:0.5em;&quot;&gt;如无法播放请更换视频源：&lt;/label&gt;
    &lt;select id=&quot;video-source&quot; onchange=&quot;switchVideoSource()&quot; style=&quot;font-size:0.9em;&quot;&gt;
      &lt;option value=&quot;youtube&quot; selected=&quot;&quot;&gt;YouTube&lt;/option&gt;
      &lt;option value=&quot;bilibili&quot;&gt;Bilibili&lt;/option&gt;
    &lt;/select&gt;
  &lt;/div&gt;
&lt;/div&gt;

&lt;script&gt;
function switchVideoSource() {
  var source = document.getElementById(&apos;video-source&apos;).value;
  var playerDiv = document.getElementById(&apos;video-player&apos;);
  if (source === &apos;bilibili&apos;) {
    playerDiv.innerHTML = &apos;&lt;iframe id=&quot;bilibili-player&quot; src=&quot;//player.bilibili.com/player.html?bvid=BV1xxxxxxx&quot; width=&quot;100%&quot; height=&quot;400&quot; frameborder=&quot;0&quot; allowfullscreen&gt;&lt;/iframe&gt;&apos;;
  } else if (source === &apos;youtube&apos;) {
    playerDiv.innerHTML = &apos;&lt;iframe id=&quot;youtube-player&quot; src=&quot;https://www.youtube.com/embed/xxxxxxxxx&quot; width=&quot;100%&quot; height=&quot;400&quot; frameborder=&quot;0&quot; allowfullscreen&gt;&lt;/iframe&gt;&apos;;
  }
}
&lt;/script&gt;

&lt;h3 id=&quot;项目demo&quot;&gt;&lt;a href=&quot;https://gitlab2.informatik.uni-wuerzburg.de/hci/software/research-prototypes/2024-ismar-identity-management-in-social-vr/-/tree/main/Application?ref_type=heads&quot; style=&quot;text-decoration:underline;&quot;&gt;项目DEMO&lt;/a&gt;&lt;/h3&gt;
&lt;hr /&gt;</content>

      
      
      
      
      

